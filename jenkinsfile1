pipeline {
  agent any

  tools {
    maven 'MVN_HOME'
    }
  
  stages {
    stage('CheckOut') {
      steps {
        echo 'Checkout the source code from GitHub'
        git 'https://github.com/sureshprojectgithub/star-agile-health-care.git'
            }
    }
    stage('Package and Build App') {
     steps {
       echo 'Build and Package App'
       sh 'mvn clean package'
           }
    }
}
}
